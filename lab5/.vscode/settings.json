{
  "git.ignoreLimitWarning": true,
  "C_Cpp.clang_format_fallbackStyle": "LLVM",
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.formatOnSave": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 100,
  "files.associations": { },
  "cpplint.filters": [
    "-whitespace",
    "-legal/copyright",
    "-readability/check",
    "-build/include_subdir",
    "-build/header_guard"
  ],
}